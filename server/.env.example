# PostgreSQL Connection Configuration
# Copy this file to .env and update with your actual credentials

# Database host (e.g., localhost, IP address, or hostname)
DB_HOST=localhost

# Database port (default PostgreSQL port is 5432)
DB_PORT=5432

# Database name
DB_NAME=your_database_name

# Database username
DB_USER=postgres

# Database password
DB_PASSWORD=your_password

# Server port for the API
PORT=3001
